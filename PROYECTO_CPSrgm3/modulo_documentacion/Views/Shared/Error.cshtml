@model ErrorViewModel
@{
    ViewData["Title"] = "Error";
    Layout = "";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <title>LARA</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <environment>
        <link href="/css/commons/lte/bootstrap.min.css" rel="stylesheet">
        <link rel="stylesheet" href="~/css/site.css" />
        <link rel="stylesheet" href="~/css/error.css" />
    </environment>
</head>
<body>
<div class="">
    <div class="">
        <div class="block-error">
            <div class="error-num">404</div>
            <div class="error-text">Page not found</div>
            <div class="error-description">
                No hemos podido encontrar la página que estás buscando. Guarda el siguiente código y prueba una de las acciones más abajo.
                @if (Model.ShowRequestId)
                {
                    <br> <br> <strong>Código de error:</strong> <code>@Model.RequestId</code>
                }
            </div>
            <div class="row">
                <div class="col-md-6">
                    <button class="btn btn-primary btn-block" onClick="history.back();">Volver</button>
                </div>
                <div class="col-md-6">
                    <button class="btn btn-info btn-block" onClick="document.location.href = '/';">Inicio</button>
                </div>
            </div>
            <div class="copy">
                © All Rights Reserved lara.com.ar 2018
            </div>
        </div>
    </div>
</div>
</body>
</html>