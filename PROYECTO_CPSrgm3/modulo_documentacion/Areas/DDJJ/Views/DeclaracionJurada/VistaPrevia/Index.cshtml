@* Familiares *@
@foreach (var item in Model.FamiliaContactoViewModel.Familiares)
{
    <div class="row">
        <div class="col-md-12">
            <box color="Primary" class="box-solid">
                <div class="box-header with-border">
                    <h3 class="box-title">DATOS DEL FAMILIAR/PERSONA A SU CARGO - VÍNCULO (@Html.DisplayFor(modelItem => item.Parentesco.Descripcion))</h3>

                </div>
                <box-body table="true">
                    <table class="table table-striped">
                        @if (item.Vive)
                        {
                            <thead>
                                <tr>
                                    <th>
                                        Apellido y nombre
                                    </th>
                                    <th>
                                        Tipo de documento @Html.DisplayFor(modelItem => item.TipoDocumento.Descripcion)
                                    </th>
                                    <th>
                                        CUIL
                                    </th>
                                    <th>
                                        Estado civil
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>
                                        @item.nombreCompleto()
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.NroDocumento)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Cuil)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.EstadoCivil.Descripcion)
                                    </td>
                                </tr>
                            </tbody>
                            <thead>
                                <tr>
                                    <th>
                                        Fecha nacimiento
                                    </th>
                                    <th>
                                        Género
                                    </th>
                                    <th>
                                        Vínculo
                                    </th>
                                    <th>
                                        Estudios cursados
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.FechaNacimiento)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Genero.Descripcion)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Parentesco.Descripcion)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Escolaridad.Descripcion)
                                    </td>
                                </tr>
                            </tbody>

                            <thead>
                                <tr>
                                    @if (item.SituacionLaboral.Id != 6)
                                    {
                                        <th>
                                            Situación laboral
                                        </th>
                                    }
                                    @if (item.SituacionLaboral.Id >= 1 && item.SituacionLaboral.Id <= 4)
                                    {
                                        <th>
                                            Fuerza a la que Pertenece
                                        </th>
                                        @if (item.SituacionLaboral.Id != 2 && item.SituacionLaboral.Id != 4)
                                        {
                                            <th>
                                                Grado
                                            </th>
                                        }
                                    }
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    @if (item.SituacionLaboral.Id != 6)
                                    {
                                        <td>
                                            @Html.DisplayFor(modelItem => item.SituacionLaboral.Descripcion)
                                        </td>
                                    }
                                    @if (item.SituacionLaboral.Id >= 1 && item.SituacionLaboral.Id <= 4)
                                    {
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Fuerza.Nombre)
                                        </td>
                                        @if (item.SituacionLaboral.Id != 2 && item.SituacionLaboral.Id != 4)
                                        {
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Grado.Nombre)
                                            </td>

                                        }
                                    }
                                </tr>
                            </tbody>
                            @if ((item.SituacionLaboral.Id >= 1 && item.SituacionLaboral.Id <= 4) || item.SituacionLaboral.Id == 6)
                            {
                                <thead>
                                    <tr>
                                        @if (item.SituacionLaboral.Id != 6)
                                        {
                                            <th>
                                                Destino
                                            </th>
                                        }
                                        else
                                        {
                                            <th>
                                                Situación laboral
                                            </th>
                                        }
                                        <th>
                                            Antiguedad
                                        </th>
                                        <th>
                                            Provincia
                                        </th>
                                        <th>
                                            Localidad
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        @if (item.SituacionLaboral.Id != 6)
                                        {
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Destino)
                                            </td>
                                        }
                                        else
                                        {
                                            <td>
                                                @Html.DisplayFor(modelItem => item.OtraSituacion)
                                            </td>
                                        }
                                        @{
                                            string tiempo;
                                            if (item.Antiguedad == 1)
                                            {
                                                tiempo = "año";
                                            }
                                            else
                                            {
                                                tiempo = "años";
                                            }
                                        }
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Antiguedad) @tiempo
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.ProvinciaEmpleador.Descripcion)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.LocalidadEmpleador.Descripcion)
                                        </td>
                                    </tr>
                                </tbody>
                            }
                            <thead>
                                <tr>
                                    <th>
                                        Posee IOSFA
                                    </th>
                                    <th>
                                        Posee alguna discapacidad
                                    </th>
                                    <th>
                                        Convive con el declarante
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.TieneIOSFA)
                                    </td>
                                    @if (item.Impedido)
                                    {
                                        if (item.Impedimiento == null)
                                        {
                                            <td>
                                                Sí
                                            </td>
                                        }
                                        else
                                        {
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Impedimiento)
                                            </td>
                                        }
                                    }
                                    else
                                    {
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Impedido)
                                        </td>
                                    }
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Convive)
                                    </td>
                                </tr>
                            </tbody>
                            @if (!item.Convive)
                            {
                                <thead>
                                    <tr>
                                        <th>
                                            Calle
