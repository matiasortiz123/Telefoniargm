@* Vivienda *@
<div class="row">
    <div class="col-sm-12">
        <box color="Primary" class="box-solid">
            <div class="box-header with-border">
                <h3 class="box-title">VIVIENDA</h3>@*Rellenar los ifs*@
            </div>
            <box-body table="true">
                @if (Model.DomicilioViviendaViewModel.Vivienda != null)
                {
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>
                                    Ocupa alojamiento en la Unidad
                                </th>
                                @if (Model.DomicilioViviendaViewModel.Vivienda.AlojaUnidad)
                                {
                                    <th>
                                        En la Unidad
                                    </th>
                                }
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => @Model.DomicilioViviendaViewModel.Vivienda.AlojaUnidad)
                                </td>
                                @if (Model.DomicilioViviendaViewModel.Vivienda.AlojaUnidad)
                                {
                                    <td>
                                        @Html.DisplayFor(modelItem => @Model.DomicilioViviendaViewModel.Vivienda.AlojamientoUnidad.Descripcion)
                                    </td>
                                }
                            </tr>
                        </tbody>
                        <thead>
                            <tr>
                                <th>
                                    Posee vivienda propia
                                </th>
                                @if (Model.DomicilioViviendaViewModel.Vivienda.ViviendaPropia)
                                {
                                    <th>
                                        Vivienda proxima a la Guarinición
                                    </th>
                                    if (Model.DomicilioViviendaViewModel.Vivienda.GuarnicionViviendaProxima2 != null)
                                    {
                                        <th>
                                            Segunda vivienda proxima a la Guarinición
                                        </th>
                                    }
                                }
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => @Model.DomicilioViviendaViewModel.Vivienda.ViviendaPropia)
                                </td>
                                @if (Model.DomicilioViviendaViewModel.Vivienda.ViviendaPropia)
                                {
                                    <td>
                                        @Html.DisplayFor(modelItem => @Model.DomicilioViviendaViewModel.Vivienda.GuarnicionViviendaProxima1.Descripcion)
                                    </td>
                                    if (Model.DomicilioViviendaViewModel.Vivienda.GuarnicionViviendaProxima2 != null)
                                    {
                                        <td>
                                            ---@Html.DisplayFor(modelItem => @Model.DomicilioViviendaViewModel.Vivienda.GuarnicionViviendaProxima2)
                                        </td>
                                    }
                                }
                            </tr>
                        </tbody>
                        <thead>
                            <tr>
                                @if (Model.DomicilioViviendaViewModel.Vivienda.ViviendaPropia && Model.DomicilioViviendaViewModel.Vivienda.GuarnicionViviendaProxima2 != null)
                                {
                                    <th>
                                        Tercera vivienda proxima a la Guarinición
                                    </th>
                                }
                                <th>
                                    Satisface sus necesitades de vivienda
                                </th>
                                @if (Model.DomicilioViviendaViewModel.Vivienda.ViviendaPropia)
                                {
                                    <th>
                                        Ocupa alguna de estas Viviendas
                                    </th>
                                }
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                @if (Model.DomicilioViviendaViewModel.Vivienda.ViviendaPropia && Model.DomicilioViviendaViewModel.Vivienda.GuarnicionViviendaProxima2 != null)
                                {
                                    <td>
                                        ---@Html.DisplayFor(modelItem => @Model.DomicilioViviendaViewModel.Vivienda.GuarnicionViviendaProxima3)
                                    </td>

                                }
                                <td>
                                    @Html.DisplayFor(modelItem => @Model.DomicilioViviendaViewModel.Vivienda.Satisface)
                                </td>
                                @if (Model.DomicilioViviendaViewModel.Vivienda.ViviendaPropia)
                                {
                                    <td>
                                        @Html.DisplayFor(modelItem => @Model.DomicilioViviendaViewModel.Vivienda.OcupaPropia)
                                    </td>
                                }
                            </tr>
                        </tbody>
                        <thead>
                            <tr>
                                <th>
                                    Alquila vivienda
                                </th>
                                @if (Model.DomicilioViviendaViewModel.Vivienda.Alquila)
                                {
                                    <th>
                                        Desde (según Contrato de Alquiler)
                                    </th>
                                    <th>
                                        Hasta (según Contrato de Alquiler)
                                    </th>
                                }
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => @Model.DomicilioViviendaViewModel.Vivienda.Alquila)
                                </td>
                                @if (Model.DomicilioViviendaViewModel.Vivienda.Alquila)
                                {
                                    <td>
                                        @Html.DisplayFor(modelItem => @Model.DomicilioViviendaViewModel.Vivienda.AlquilaDesde)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => @Model.DomicilioViviendaViewModel.Vivienda.AlquilaHasta)
                                    </td>
                                }
                            </tr>
                        </tbody>
                        <thead>
                            <tr>
                                <th>
                                    Ocupa vivienda del estado
                                </th>
                                @if (Model.DomicilioViviendaViewModel.Vivienda.OcupaViviendaEstado)
                                {
                                    <th>
                                        Hace cuanto tiempo
                                    </th>
                                }
                                <th>
                                    Posee crédito de vivienda
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => @Model.DomicilioViviendaViewModel.Vivienda.OcupaViviendaEstado)
                                </td>
                                @if (Model.DomicilioViviendaViewModel.Vivienda.OcupaViviendaEstado)
                                {
                                    string tiempo;
                                    if (Model.DomicilioViviendaViewModel.Vivienda.TiempoOcupaVivienda == 1)
                                    {
                                        tiempo = "año";
                                    }
                                    else
                                    {
                                        tiempo = "años";
                                    }

                                    <td>
                                        @Html.DisplayFor(modelItem => @Model.DomicilioViviendaViewModel.Vivienda.TiempoOcupaVivienda) @tiempo
                                    </td>
                                }
                                <td>
                                    @Html.DisplayFor(modelItem => @Model.DomicilioViviendaViewModel.Vivienda.PoseeCreditoVivienda)
                                </td>
                            </tr>
                        </tbody>
                        <thead>
                            <tr>
                                <th>
                                    Construye Vivienda
                                </th>
                                @if (Model.DomicilioViviendaViewModel.Vivienda.ConstruyeVivienda)
                                {
                                    <th>
                                        Fecha estimada de Finalización
                                    </th>
                                }
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => @Model.DomicilioViviendaViewModel.Vivienda.ConstruyeVivienda)
                                </td>
                                @if (Model.DomicilioViviendaViewModel.Vivienda.ConstruyeVivienda)
                                {
                                    <td>
                                        @Html.DisplayFor(modelItem => @Model.DomicilioViviendaViewModel.Vivienda.FechaConstruye)
                                    </td>
                                }
                            </tr>
                        </tbody>
                    </table>
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>
                                    Además del lugar donde posee vivienda propia, en que guarnición soluciona por propios medios su situación de vivienda
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => @Model.DomicilioViviendaViewModel.Vivienda.GuarnicionSoluciona.Descripcion)
                                </td>
                            </tr>
                        </tbody>
                        <thead>
                            <tr>
                                <th>
                                    Situación economica que desea resaltar
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                @if (Model.DomicilioViviendaViewModel.Vivienda.SituacionEconomica == null)
                                {
                                    <td>
                                        Ninguna
                                    </td>
                                }
                                else
                                {
                                    <td>
                                        @Html.DisplayFor(modelItem => @Model.DomicilioViviendaViewModel.Vivienda.SituacionEconomica)
                                    </td>
                                }
                            </tr>
                        </tbody>
                    </table>
                }
                else
                {
                    <table class="table table-striped">
                        <thead></thead>
                        <tbody>
                            <tr>
                                <td class="text-danger" colspan="3" style="text-align:center">
                                    NO SE ENCONTRARON REGISTROS
                                </td>
                            </tr>
                        </tbody>
                    </table>
                }

            </box-body>
        </box>
    </div>
</div>

